    background-color: rgba(153,153,153,0.01);
    border-color: #ffffff;
    border-width: 2px;
    text-align: center;
  }
  .thim-button--rayen {
    width: 100%;
    overflow: hidden;
    padding: 0;
}
  .thim-button--border-thick {
    border: 3px solid;
}
.thim-button--border-thick {
          border: 3px solid;
      }
.thim-button {
    float: left;
    display: block;
    padding: 0.5em 2em;
    border: none;
    background: none;
    color: inherit;
    vertical-align: middle;
    position: relative;
    z-index: 1;
    -webkit-backface-visibility: hidden;
    -moz-osx-font-smoothing: grayscale;
}

