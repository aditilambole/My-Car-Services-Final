                                    </a>

                                </h1>
                            </div>
                            <nav class="width-navigation table-cell table-right col-md-3 col-md-offset-2">
                                <div class="inner-navigation container">
                                    <ul class="nav navbar-nav fixed-top menu-main-menu">
                                        @if (ViewContext.HttpContext.User.IsInRole("Admin"))
                                        {
                                            <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-home current-menu-item page_item page-item-14 current_page_item drop_to_right standard">
                                                @Html.ActionLink("Home", "Index", "Home", " ",new { @class = "anchorNav "})
